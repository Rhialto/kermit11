;	K11IDM.ODL	I/D odl for M+

	.psect	topmem	,d,rw,gbl,ovr,rel
	.name	utl
	.name	err
	.name	sr
	.root	rm-rmsrot,senrec,utilty,error,topmem
rm:	.fctr	a-b-c-d-e-f-errdvr-mem-sub
a:	.fctr	lb:k11pak
b:	.fctr	lb:k11dat
c:	.fctr	lb:k11rms
d:	.fctr	lb:k11m41
e:	.fctr	lb:k11cmd
f:	.fctr	lb:k11lcl-lb:k11edi

senrec:	.fctr	sr-*(lb:k11sen,lb:k11rec,lb:k11cm1)

set0:	.fctr	lb:k11st0
set1:	.fctr	lb:k11st1
show:	.fctr	lb:k11sho
connec:	.fctr	lb:k11pco
xcon:	.fctr	lb:k11mco
help:	.fctr	lb:k11hlp
server:	.fctr	lb:k11ser
transf:	.fctr	lb:k11tra
